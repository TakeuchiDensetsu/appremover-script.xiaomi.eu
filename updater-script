delete("/system/app/MiuiVideo/MiuiVideo.apk");
ui_print("Removing Notes ...");
delete("/system/app/Notes/Notes.apk");
ui_print("Removing BugReport ...");
delete("/system/app/BugReport/BugReport.apk");
ui_print("Removing MiuiVideoGlobal ...");
delete("/system/app/MiuiVideoGlobal/MiuiVideoGlobal.apk");

