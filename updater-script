delete("/system/priv-app/Calender/Calender.apk");
ui_print("Removing ThemeManager ...");
delete("/system/priv-app/ThemeManager/ThemeManager.apk");
ui_print("Removing Updater ...");
delete("/system/priv-app/Updater/Updater.apk");

set_progress(0.6);
ui_print("Removing app ...");
ui_print("Removing Calculator ...");
delete("/system/app/Calculator/Calculator.apk");
ui_print("Removing Mail ...");
delete("/system/app/Email/Email.apk");
ui_print("Removing Explorer ...");
delete("/system/app/FileExplorer/FileExplorer.apk");
