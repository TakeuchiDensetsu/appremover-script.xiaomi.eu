ui_print("");
ui_print("Remove Apps ...");
show_progress(1.0,0);
ui_print("Mounting system...");
run_program("/sbin/mount", "/system");

set_progress(0.1);
ui_print("Deleting priv-app ...");

set_progress(0.6);
ui_print("Deleting app ...");

set_progress(0.9);
ui_print("Unmounting system...");
run_program("/sbin/umount", "/system");
set_progress(1.00);
ui_print("Complete!");